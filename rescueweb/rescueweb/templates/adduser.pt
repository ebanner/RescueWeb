<div metal:use-macro="main.macros['template']">
    <div metal:fill-slot="content">
    
        <form action="/adduser" method="post">
			<div tal:repeat = "header headers">
				${header}: <input type="text" name="${header} value="${header}"/><br/>
			</div>
			
			privileges: <select name="privileges">
						  	<div tal:repeat = "priv privilegesOptions">
  						   		<option value=${priv}>${priv}</option>
  							</div>
						  </select><br/>
						  
			trainingvalue: <select name="trainingvalue">
								<div tal:repeat = "train trainingOptions">
  						   			<option value=${train}>${train}</option>
  								</div>
						    </select><br/>
						    
			administrativevalue: <select name="administrativevalue">
								<div tal:repeat = "admin administrativeOptions">
  						   			<option value=${admin}>${admin}</option>
  								</div>
						    </select><br/>
						    
			operationalvalue: <select name="operationalvalue">
								<div tal:repeat = "oper operationalOptions">
  						   			<option value=${oper}>${oper}</option>
  								</div>
						    </select><br/>	    
			
			
          <input type="submit" name="form.submitted" value="Add User"/>
        </form>
    </div>
</div>